1950 rem **** COMBINE DATA FOR J&J ****
1960 for j=1 to nj
1970 a(j)=0:b(j)=0
1980 next J
1990 for i=1 to kk 
2000 for j=1 to nj
2010 a(j)=a(j)+m(j,i)
2020 b(j)=b(j)+m(j,nm-i+1)
2030 next J
2040 next I
2050 return
2060 rem **** J&J ALGORITHM ****
2070 nf=0:nl=nj: l=0
2080 gosub 2360
2090 for j=1 to nj
2100 c(j)=nj+1
2110 next J
2120 sm=tt 
2130 for j=1 to nj
2140 fl=0
2150 rem **** ELIMINATE DONE JOBS ****
2160 for jj=1 to nj 
2170 if j=c(jj) then fl=1
2180 next jj
2190 if fl=1 then goto 2220
2200 if b(j)<= sm then sm=b(j):il=j:l=2
2210 if a(j)<= sm then am=a(j):il=j:l=0
2220 next J
2230 if l=2 then goto 2260
2240 c(nf)=il
2250 nf=nf+1:goto 2280
2260 c(nl)=il 
